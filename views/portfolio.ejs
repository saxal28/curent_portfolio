<% include /partials/header %>

<div class="container-fluid portfolio-header">
        <h1 class="title">Portfolio</h1>
</div>

<div class="container">
        <div class="row">

<% portfolios.forEach(function(item) { %>

        <div class="col-md-12">
            <div class="thumbnail">
                <img src="<%=item.image%>">
                <div class="caption text-center">
                    <h4><%=item.title%></h4>
                    <a href="/portfolio/<%=item._id%>">Show</a>
                </div>
            </div>
        </div>
    
<% }); %>

    </div>

</div>




<% include /partials/footer %>

