    <section class="header" id="home">
        <h1 id="title" class="animated bounceInLeft">Alan Sax</h1>
        <h3 id="title-subtitle" class="animated bounceInRight">Front End Developer</h3>
    </section>
    
    
    <section id="skills">
       <div class="row row-centered" style="width:100%; margin:0 auto">
           <div class="col-md-12" id="skills-row-left">
               <div class="card skills-card">
                   <div class="card-block text-center">
                        <img src="https://i1.wp.com/pbs.twimg.com/media/Cyhe1gkXUAAl58E.jpg?w=840&ssl=1" class="icon" style="width:150px; height:100px"/>
                        <img src="http://nodeprogram.com/assets/images/enm.jpg" class="icon" style="height:100px; width:150px"/>
                        <img src="https://pixels4kids.files.wordpress.com/2014/11/html_css_javascript_and_the_awesome_jquery_infographic_header.png?w=1200" class="icon" style="width:150px; height:100px"/>
                        <img src="https://aptonic.com/blog/wp-content/uploads/2015/08/python-logo.png" class="icon" style="width:100px; height:100px" />
                        
                   </div>
               </div>
           </div>
           
       </div>
    </section>
    
    <section id="portfolio">
        <h2 id="portfolio-title">Projects</h2>
        <div class="row">
            <% portfolio.forEach(function(portfolio) { %>
            <div class="col-lg-4 col-md-6 portfolio-images">
                <a href="<%= portfolio.link %>" target="_blank"><img src='<%=portfolio.image%>' class="portfolio-image" /></a>
            </div>
            <% }) %>
        </div>
       
    </section>
    
    <section id="contact">
        <h2 class="contact-header">Contact Me</h2>
        <div class="row contact-row">
            <a href="https://www.facebook.com/alan.sax" class="btn btn-outline-secondary"target="_blank">Facebook</a>
            <a href="https://github.com/saxal28" class="btn btn-outline-secondary"target="_blank">Github</a>
            <a href="https://www.linkedin.com/in/alan-sax-576a24b3" class="btn btn-outline-secondary" target="_blank">LinkedIn</a>
            <a href="https://www.freecodecamp.com/saxal28" class="btn btn-outline-secondary"target="_blank">FreeCodeCamp</a>
        </div>
        <div class="row contact-row">
            <!--when updating resume, keep same name-->
            <a href="https://github.com/saxal28/caesar_cipher/raw/master/Resume_Sax_Alan.pdf"class="btn btn-outline-secondary">Download My Resume</a>
        </div>
    </section>



