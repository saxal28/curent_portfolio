       </div>
    </section>
    
    <section id="portfolio">
        <h2>Portfolio</h2>
        <div class="row">
           <div class="card-group">
               
               
            <% portfolio.forEach(function(portfolio) { %>
            <div class="col-lg-4">
            
             <div class="card card-portfolio">
                <img class="card-img-top" src="<%=portfolio.image%>" alt="Card image cap">
                <div class="card-block">
                  <h5 class="card-title" style="height:50px"><%=portfolio.title%></h5>
                  <p class="card-text" style="height:100px"><%=portfolio.description%></p>
                  <div class="row button-row">
                      <a href="<%=portfolio.link%>" class="btn btn-outline-secondary">Play</a>
                      <a  href="<%=portfolio.github%>" class="btn btn-outline-secondary">View Code</a>
                  </div>
                </div>
              </div>
              
              </div>
                
            <% }) %>
              
            </div>
        </div>
    </section>
    
